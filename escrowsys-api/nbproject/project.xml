<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/3">
            <name>escrowsys-api</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <web-module-libraries>
                <library>
                    <file>${reference.escrowsys-core.dist}</file>
                </library>
                <library>
                    <file>${reference.escrowsys-dto.jar}</file>
                </library>
                <library>
                    <file>${reference.escrowsys-helper.jar}</file>
                </library>
                <library>
                    <file>${libs.J5-JBOSS-CLIENT.classpath}</file>
                </library>
                <library>
                    <file>${libs.J5-FREEMARKER.classpath}</file>
                </library>
                <library>
                    <file>${libs.J5-ESAPI.classpath}</file>
                </library>
                <library>
                    <file>${libs.J5-HIBERNATE.classpath}</file>
                </library>
                <library>
                    <file>${libs.J5-APACHE-COMMONS.classpath}</file>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>escrowsys-core</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>dist</target>
                <clean-target>clean</clean-target>
                <id>dist</id>
            </reference>
            <reference>
                <foreign-project>escrowsys-dto</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>escrowsys-helper</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
    </configuration>
</project>
